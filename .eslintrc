{
  "env": {
    "es6": true,
    "node": true,
    "mocha": true,
    "browser": true
  },
  "plugins": [
    "html"
  ],
  "parserOptions": {
    "sourceType": "module"
  },
  "globals": {
    "weex": true,
    "Vue": true
  },
  "rules": {
    "semi": "off",
    "indent": 0,
    "quotes": [ "error", "single" ],
    "no-control-regex": 0,
    "no-extend-native": 0,
    "no-return-assign": [ 2, "except-parens" ],
    "space-before-function-paren": 0,
    "object-curly-spacing": 0,
    "prefer-const": 0,
    "one-var": 0
  },
  "extends": [
    "vue"
  ]
}
